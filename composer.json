{
  "name": "shamithewebdeveloper/laravel-forms",
  "description": "A Laravel package for easy HTML form generation.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ehtisham Asghar",
      "email": "ehtishamasghar227@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "ShamiTheWebdeveloper\\LaravelForms\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "ShamiTheWebdeveloper\\LaravelForms\\FormServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
